      <samplerate>44100</samplerate>
      <buffersize>4096</buffersize>
      <padzero>4</padzero>
    </device>
    <device id="l34"  xsi:type="apex:L34DeviceType">
      <device_id>1</device_id>
      <implant>$$implant$$</implant>
      <trigger>in</trigger>
      <volume id='l34volume'>$$volume$$</volume>
      
